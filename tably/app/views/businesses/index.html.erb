
<h1>Businesses</h1>

<% if @businesses %>

	<% @businesses.each do |business| %>
		<ul>
			
			<li> <%= link_to business.name, business_path(business)%> </li>

		</ul>
	
	<% end %>

<% end %>

<% @parsed['businesses'].each do |biz| %>
	
	<p><a href="/businesses/<%=biz['id']%>"><%=biz['name']%></a></p>
	<img src="<%=biz['image_url']%>" alt="">
	<p><%= biz['location'] %></p>

<% end %>